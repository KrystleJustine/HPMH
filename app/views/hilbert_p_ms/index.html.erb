<p id="notice"><%= notice %></p>

<h1>Hilbert Pms</h1>

<table>
  <thead>
    <tr>
      <th>Name</th>
      <th>Location</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @hilbert_pms.each do |hilbert_pm| %>
      <tr>
        <td><%= hilbert_pm.name %></td>
        <td><%= hilbert_pm.location %></td>
        <td><%= link_to 'Show', hilbert_pm %></td>
        <td><%= link_to 'Edit', edit_hilbert_pm_path(hilbert_pm) %></td>
        <td><%= link_to 'Destroy', hilbert_pm, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Hilbert Pm', new_hilbert_pm_path %>
